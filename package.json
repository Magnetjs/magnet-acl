{
  "name": "magnet-acl",
  "version": "0.3.0",
  "description": "Magnet adapter for acl",
  "keywords": [
    "magnet",
    "magnetjs",
    "acl"
  ],
  "homepage": "https://github.com/magnetjs/magnet-acl#readme",
  "bugs": {
    "url": "https://github.com/magnetjs/magnet-acl/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": "magnetjs/magnet-acl",
  "scripts": {
    "build": "tsc",
    "dist": "npm run build && npm publish",
    "start": "tsc",
    "test": "ts-node tests/index.js | tap-spec",
    "watch": "tsc - w",
    "watch-test": "nodemon --delay 0.5 --ignore src --exec ts-node tests/index.js | tap-spec",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\""
  },
  "dependencies": {
    "acl": "^0.4.10",
    "magnet-core": "^3.3.0"
  },
  "devDependencies": {
    "fly": "^2.0.5",
    "fly-clear": "^1.0.1",
    "fly-esnext": "^2.0.1",
    "fly-typescript": "^1.0.0",
    "fly-watch": "^1.1.1"
  },
  "engines": {
    "node": ">=6.9.5"
  }
}
